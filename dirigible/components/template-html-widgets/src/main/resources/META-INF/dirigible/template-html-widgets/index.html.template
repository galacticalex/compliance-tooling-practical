	<body fd-scrollbar dg-contextmenu="contextMenuContent">
		<fd-button dg-label="Reload view" onClick="window.location.reload();"></fd-button>
		<h4>Buttons</h4>
		<fd-button dg-label="Change state" ng-click="state()"></fd-button>

		<fd-button dg-label="{{ btnText }}" badge="121" compact="true" glyph="sap-icon--folder" state="{{ btnState }}"
			dg-aria-desc="Just testing stuff" dg-type="{{ btnType }}" ng-click="test()">
		</fd-button>

		<fd-button dg-label="Selection menu" is-menu="true" dg-aria-desc="An ordinary description of a button.">
		</fd-button>

		<fd-button glyph="sap-icon--folder" aria-label="Folder" ng-click="setTristate()">
		</fd-button>

		<fd-segmented-button aria-label="Test group">
			<fd-button dg-label="Left" state="{{ segmentedModel == 'left' ? 'selected' : '' }}"
				ng-click="segmentedClick('left')">
			</fd-button>
			<fd-button dg-label="Middle" state="{{ segmentedModel == 'middle' ? 'selected' : '' }}"
				ng-click="segmentedClick('middle')">
			</fd-button>
			<fd-button dg-label="Right" state="{{ segmentedModel == 'right' ? 'selected' : '' }}"
				ng-click="segmentedClick('right')">
			</fd-button>
		</fd-segmented-button>

		<fd-split-button main-action="{{ splitButtonAction }}" dg-type="emphasized" aria-label="button-split"
			callback="splitButtonClick">
			<fd-menu aria-label="split button items">
				<fd-menu-item title="Split item" ng-click="splitItemClick('Split item')"></fd-menu-item>
				<fd-menu-item title="Split item 2" ng-click="splitItemClick('Split item 2')"></fd-menu-item>
				<fd-menu-item title="Split item 3" ng-click="splitItemClick('Split item 3')"></fd-menu-item>
			</fd-menu>
		</fd-split-button>

		<fd-popover dg-align="right">
			<fd-popover-control>
				<fd-button dg-label="Popover scroll">
				</fd-button>
			</fd-popover-control>
			<fd-popover-body can-scroll="true" dg-align="right" max-height="200">
				<fd-menu aria-label="popover items">
					<fd-menu-item title="Specal item" ng-click="popoverItemClick()"></fd-menu-item>
					<fd-menu-separator></fd-menu-separator>
					<fd-menu-item title="Some option" ng-click="popoverItemClick()"></fd-menu-item>
